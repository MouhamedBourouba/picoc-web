{
  "name": "picoc-web",
  "version": "1.0.2",
  "description": "Run C code in the web using PicoC + Emscripten.",
  "main": "dist/bundle.umd.js",
  "browser": "dist/bundle.iife.js",
  "module": "dist/bundle.esm.js",
  "exports": {
    ".": {
      "import": "./dist/bundle.esm.js",
      "require": "./dist/bundle.umd.js",
      "browser": "./dist/bundle.iife.js"
    }
  },
  "scripts": {
    "build": "emmake make -j8 && rollup -c",
    "clean": "emmake make clean && rm dist -rf"
  },
  "keywords": [
    "c",
    "wasm",
    "picoc",
    "emscripten",
    "webassembly"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.47.1"
  }
}
